{
    "swagger": "2.0",
    "info": {
        "description": "API untuk mengekstrak informasi splitbill dari gambar struk menggunakan OCR dan AI",
        "title": "Splitbill API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0"
    },
    "host": "localhost:3000",
    "basePath": "/",
    "paths": {
        "/": {
            "post": {
                "description": "Upload a receipt image and extract detailed splitbill information including items, store details, totals, and transaction information using OCR and AI",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Splitbill"
                ],
                "summary": "Extract splitbill information from receipt image",
                "parameters": [
                    {
                        "type": "file",
                        "description": "Receipt image file (jpg, jpeg, png)",
                        "name": "image",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "202": {
                        "description": "Successfully processed receipt",
                        "schema": {
                            "$ref": "#/definitions/models.SplitbillResponse"
                        }
                    },
                    "406": {
                        "description": "Failed to process receipt",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.ErrorResponse": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "string"
                },
                "status": {
                    "type": "string",
                    "example": "Error uploading image"
                }
            }
        },
        "models.Item": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string",
                    "example": "Nasi Goreng"
                },
                "price": {
                    "type": "string",
                    "example": "25000.00"
                },
                "quantity": {
                    "type": "string",
                    "example": "2"
                },
                "total": {
                    "type": "string",
                    "example": "50000.00"
                }
            }
        },
        "models.SplitbillResponse": {
            "type": "object",
            "properties": {
                "items": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Item"
                    }
                },
                "store_information": {
                    "$ref": "#/definitions/models.StoreInformation"
                },
                "totals": {
                    "$ref": "#/definitions/models.Totals"
                },
                "transaction_information": {
                    "$ref": "#/definitions/models.TransactionInfo"
                }
            }
        },
        "models.StoreInformation": {
            "type": "object",
            "properties": {
                "address": {
                    "type": "string",
                    "example": "Jl. Sudirman No. 123, Jakarta"
                },
                "email": {
                    "type": "string",
                    "example": "info@restaurant.com"
                },
                "npwp": {
                    "type": "string",
                    "example": "12.345.678.9-012.345"
                },
                "phone_number": {
                    "type": "string",
                    "example": "+62812345678"
                },
                "store_name": {
                    "type": "string",
                    "example": "Restaurant ABC"
                }
            }
        },
        "models.Tax": {
            "type": "object",
            "properties": {
                "amount": {
                    "type": "string",
                    "example": "5000.00"
                },
                "dpp": {
                    "type": "string",
                    "example": "95000.00"
                },
                "name": {
                    "type": "string",
                    "example": "PPN"
                },
                "service_charge": {
                    "type": "string",
                    "example": "0.00"
                },
                "total_tax": {
                    "type": "string",
                    "example": "5000.00"
                }
            }
        },
        "models.Totals": {
            "type": "object",
            "properties": {
                "change": {
                    "type": "string",
                    "example": "5000.00"
                },
                "discount": {
                    "type": "string",
                    "example": "0.00"
                },
                "payment": {
                    "type": "string",
                    "example": "105000.00"
                },
                "subtotal": {
                    "type": "string",
                    "example": "95000.00"
                },
                "tax": {
                    "$ref": "#/definitions/models.Tax"
                },
                "total": {
                    "type": "string",
                    "example": "100000.00"
                }
            }
        },
        "models.TransactionInfo": {
            "type": "object",
            "properties": {
                "date": {
                    "type": "string",
                    "example": "02/08/2025"
                },
                "time": {
                    "type": "string",
                    "example": "19:30"
                },
                "transaction_id": {
                    "type": "string",
                    "example": "TXN123456789"
                }
            }
        }
    }
}